{
  "name": "impeditqui",
  "version": "1.0.0",
  "description": "micromark extension to support MDX or MDX.js JSX",
  "license": "MIT",
  "keywords": [
    "micromark",
    "micromark-extension",
    "autem",
    "mdxjs",
    "jsx",
    "js",
    "ullam",
    "corrupti",
    "voluptas",
    "id",
    "aut"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AndyCassin/impeditqui.git"
  },
  "bugs": {
    "url": "https://github.com/AndyCassin/impeditqui.git/issues"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "CorineLang",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dev/",
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "exports": {
    "development": "./dev/index.js",
    "default": "./index.js"
  },
  "dependencies": {
    "@types/acorn": "^4.0.0",
    "estree-util-is-identifier-name": "^2.0.0",
    "micromark-factory-mdx-expression": "^1.0.0",
    "micromark-factory-space": "^1.0.0",
    "micromark-util-character": "^1.0.0",
    "micromark-util-symbol": "^1.0.0",
    "micromark-util-types": "^1.0.0",
    "uvu": "^0.5.0",
    "vfile-message": "^3.0.0"
  },
  "devDependencies": {
    "@types/mdast": "^3.0.0",
    "@types/tape": "^4.0.0",
    "acorn": "^8.0.0",
    "acorn-jsx": "^5.0.0",
    "c8": "^7.0.0",
    "mdast-zone": "^5.0.0",
    "micromark": "^3.0.0",
    "micromark-build": "^1.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^10.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "rimraf": "^3.0.0",
    "tape": "^5.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "xo": "^0.47.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "node/file-extension-in-import": "off",
      "unicorn/no-this-assignment": "off",
      "unicorn/prefer-code-point": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      "./script/grammar.js"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": true
  },
  "homepage": "https://github.com/AndyCassin/impeditqui.git"
}